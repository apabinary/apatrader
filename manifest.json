{
  "name": "apatrader",
  "short_name": "apatrader",
  "version": "3.1.1",
  "manifest_version": 2,
  "description": "Webtrader for Binary.com",
  "homepage_url": "https://apatrade.github.io/apatrader",
  "icons": {
    "16": "v3.1.1/images/favicons/favicon-16x16.png",
    "32": "v3.1.1/images/favicons/favicon-32x32.png",
    "96": "v3.1.1/images/favicons/favicon-96x96.png",
    "128": "v3.1.1/images/favicons/favicon-128x128.png",
    "160": "v3.1.1/images/favicons/favicon-160x160.png",
    "192": "v3.1.1/images/favicons/favicon-192x192.png"
  },
  "background":{
    "scripts": ["chrome_background.js"]
  },
  "browser_action": {
    "default_icon": "v3.1.1/images/favicons/favicon-128x128.png",
    "default_title": "Webtrader for Binary.com"
  },
  "permissions": [
    "cookies", "tabs",
    "https://*.binary.com/*"
  ]
}
